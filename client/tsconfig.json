{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "rootDir": "./",
    "outDir": "dist",
    "allowJs": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      // "@/*": ["src/*"],
      "@pages/*": ["src/pages/*"],
      "@atoms/*": ["src/components/atoms/*"],
      "@molecules/*": ["src/components/molecules/*"],
      "@organisms/*": ["src/components/organisms/*"],
      "@styles/*": ["src/styles/*"],
      "@hooks/*": ["./src/hooks/*"]
    },
    "skipLibCheck": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
